<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>

<?import javafx.scene.image.Image?>
<fx:root fx:id="boite" xmlns="http://javafx.com/javafx/8.0.121"
         xmlns:fx="http://javafx.com/fxml/1"
         type="javafx.scene.layout.Pane">
    <fx:define>
        <Image fx:id="cercle" url="@../o.png"/>
        <Image fx:id="croix" url="@../x.png"/>
    </fx:define>

    <ImageView fx:id="imageView" preserveRatio="true" fitHeight="${boite.height}" fitWidth="${boite.width}"/>
</fx:root>
